{
	"name": "TermiPass",
	"description": "DID APP",
	"version": "1.0.0",
	"manifest_version": 3,
	"icons": {
		"16": "icons/icon-16x16.png",
		"48": "icons/icon-48x48.png",
		"128": "icons/icon-128x128.png"
	},
	"action": {
		"default_title": "vault"
	},
	"background": {
		"service_worker": "background.js",
		"mode": "module"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"www/js/webextension-polyfill.js",
				"www/js/bex-content-script.js"
			],
			"run_at": "document_start"
		},
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"www/js/webextension-polyfill.js",
				"js/content-script.js"
			]
		},
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"www/js/webextension-polyfill.js",
				"www/js/app.js",
				"www/js/autofill.js",
				"www/js/TermiPassProvider.js",
				"www/js/rssContentScript.js"
			],
			"css": [
				"www/css/app.css",
				"css/content-css.css",
				"css/content-css-modify.css",
				"css/content-css-app-root.css",
				"css/content-css-dialog-content.css",
				"css/content-css-q-notify.css",
				"css/content-css-menu-content.css"
			]
		}
	],
	"permissions": [
		"storage",
		"scripting",
		"alarms",
		"unlimitedStorage",
		"activeTab",
		"notifications",
		"contextMenus",
		"clipboardRead",
		"clipboardWrite",
		"cookies"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"www/*",
				"js/*",
				"css/*"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
	}
}
